{
    "autoload": {
        "psr-4": {
            "MauticPlugin\\MauticSevenBundle\\": ""
        }
    },
    "description": "Mautic Plugin for sending SMS via seven.io",
    "extra": {
        "install-directory-name": "MauticSevenBundle"
    },
    "name": "seven.io/mautic",
    "require": {
        "php": ">=8.0 <8.1",
        "seven.io/api": "v4.0.0",
        "mautic/core-lib": "*",
        "mautic/composer-plugin": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "symplify/easy-coding-standard": "^12.0"
    },
    "scripts": {
        "csfixer": "../../bin/php-cs-fixer fix . -v --dry-run --diff --using-cache=no",
        "fixcs": "../../bin/php-cs-fixer fix . -v --using-cache=no"
    },
    "type": "mautic-plugin",
    "version": "0.0.1"
}
